{
	"name": "lavender-commerce/lavender",
	"description": "Lavender",
	"keywords": ["ecommerce", "lavender", "nsfw"],
	"license": "MIT",
    "authors": [
        {
            "name": "Daniel sloof",
            "email": "daniel@rubic.nl"
        },
        {
            "name": "Ryan Tulino",
            "email": "rtulino@gmail.com"
        }
    ],
	"require": {
        "php": ">=5.4.0",
		"laravel/framework": "4.2.*"
	},
	"autoload": {
		"classmap": [
            "app/lib",
            "app/controllers",
            "app/composers",
            "app/models",
            "app/database/migrations",
			"app/database/seeds"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}

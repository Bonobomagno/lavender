{
    "name": "lavender-commerce/lavender",
    "description": "Lavender",
    "keywords": ["lavender", "ecommerce"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Tulino",
            "email": "rtulino@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lavender-commerce/config"
        },
        {
            "type": "vcs",
            "url": "https://github.com/lavender-commerce/entity"
        },
        {
            "type": "vcs",
            "url": "https://github.com/lavender-commerce/account"
        },
        {
            "type": "vcs",
            "url": "https://github.com/lavender-commerce/cms"
        },
        {
            "type": "vcs",
            "url": "https://github.com/lavender-commerce/workflow"
        },
        {
            "type": "vcs",
            "url": "https://github.com/lavender-commerce/catalog"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ryaan-anthony/lavender-contact-form"
        }
    ],
    "require": {
        "php":                          ">=5.4.0",
        "laravel/framework":            "4.2.*",
        "lavender-commerce/config":     "dev-master",
        "lavender-commerce/entity":     "dev-master",
        "lavender-commerce/account":    "dev-master",
        "lavender-commerce/cms":        "dev-master",
        "lavender-commerce/workflow":   "dev-master",
        "lavender-commerce/catalog":    "dev-master",
        "ryaan-anthony/lavender-contact-form":    "dev-master"
    },
    "autoload": {
        "classmap": [
            "app/lib",
            "app/database/migrations",
            "app/database/seeds"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
